<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/relativelayout_main_rules"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="vertical"
    android:parentActivityName="LoginAcitivty"
    tools:context="com.example.project.activity_fragments_class.RulesActivity">

    <Toolbar
        android:id="@+id/toolbar_activityRules"
        android:background="@color/colorDellyGradientBlue"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </Toolbar>

    <WebView
        android:layout_below="@id/toolbar_activityRules"
        android:id="@+id/webview_activityRules"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </WebView>



</RelativeLayout>
