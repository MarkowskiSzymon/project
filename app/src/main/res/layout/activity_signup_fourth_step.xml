<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.project.activity_fragments_class.Registration.SignupActivity_FourthStep">


    <Button
        android:id="@+id/przelaczAnimacje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Przelacz animacje"/>

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/animacjaFourth"
        android:layout_below="@id/przelaczAnimacje"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        app:lottie_autoPlay="false"
        app:lottie_loop="true"
        app:lottie_fileName="412-download-icon-success.json"
        android:transitionName="example_transistion"/>


</RelativeLayout>
